DATABASE_URL=postgresql+psycopg://postgres:postgres@localhost:5432/central_achat_publique
POSTGRES_PORT=5432
ALLOW_DB_DRIFT=0
APP_ENV=dev
MISTRAL_API_KEY=<MISTRAL_API_KEY>
MISTRAL_MODEL=mistral-large-latest
DISCOVERY_COMPARE_LLM_ENABLED=1
DISCOVERY_COMPARE_AGENT_VERSION_MODE=auto
DISCOVERY_COMPARE_LLM_TIMEOUT_SECONDS=30
DISCOVERY_COMPARE_SNAPSHOT_PROVIDER=playwright
DISCOVERY_COMPARE_SNAPSHOT_REQUIRE=1
PLAYWRIGHT_MCP_MODE=http
PLAYWRIGHT_MCP_COMMAND=npx
PLAYWRIGHT_MCP_ARGS=@playwright/mcp@latest
PLAYWRIGHT_MCP_CWD=
PLAYWRIGHT_MCP_URL=
PLAYWRIGHT_MCP_TIMEOUT_SECONDS=30 
PLAYWRIGHT_MCP_INSTALL=1
DISCOVERY_COMPARE_SNAPSHOT_SCREENSHOT_ENABLED=0
DISCOVERY_COMPARE_SNAPSHOT_MAX_BYTES=1000000
DISCOVERY_COMPARE_SNAPSHOT_USER_AGENT=
DISCOVERY_COMPARE_PRODUCT_CANDIDATE_PROVIDER=stub

# Exa API key for MCP endpoint (optional in CI, required for real Exa MCP usage)
EXA_API_KEY=<EXA_API_KEY>
EXA_MCP_URL=https://mcp.exa.ai/mcp
EXA_MCP_TIMEOUT_SECONDS=20
EXA_MCP_LIMIT=8

